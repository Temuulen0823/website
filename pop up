      function Signin() {
        let email = document.getElementById("email").value;
        let password = document.getElementById("password").value;
        let correct_password = "nest0000";
        let correct_email = "nest@gmail.com";
        if (password === correct_password) {
          alert("pass zov");
        } else {
          alert("pass buruu bn");
        }
        if (email === correct_email) {
          alert("email zov");
        } else {
          alert(" email buruu bn");
        }
      }
    </script>
